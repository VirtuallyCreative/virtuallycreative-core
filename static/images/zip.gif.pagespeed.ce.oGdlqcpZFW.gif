			 .ips_editor.with_sidebar .sidebar h4 span {
				padding: 6px 0 0 6px;
				display: block;
			}
		
		 .ips_editor.with_sidebar .sidebar .emoticon_holder {
			position: absolute;
			top: 25px;
			bottom: 1px;
			width: 193px;
			overflow: auto;
		}
		

	 .ips_editor .toolbar {
		height: 30px;
