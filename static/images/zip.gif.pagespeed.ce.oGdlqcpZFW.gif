	font-size: 0.9em;
	min-width: 85px;
	padding: 5px;
	z-index: 2000;
}
	
	.ipbmenu_content li, .ipb_autocomplete li {
		padding: 5px;
	}
	
	.ipbmenu_content li:last-child {
		border-bottom: 0;
		padding-bottom: 0px;
	}
	
	.ipbmenu_content li:first-child {
		padding-top: 0px;
	}
	
	.ipbmenu_content a {
		text-decoration: none;
