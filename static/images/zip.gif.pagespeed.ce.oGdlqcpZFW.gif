	width: 25%;
}
.col_c_stats {
	width: 15%;
}
.col_c_post {
	width: 30%;
}
.col_c_forum {
	width: 55%;
}
.col_mod, .col_f_mod, .col_r_mod {
	width: 3%;
}
.col_r_title {
	width: 26%;
}

