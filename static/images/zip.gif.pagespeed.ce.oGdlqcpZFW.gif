	}

		#rss_menu a:hover {
			background-color: #ff7600;
			color: #fff;
		}
		
#pm_notification {
	background: url(https://dreamincode.net/forums/public/style_images/master/trans_bg.png);
	color: #515151;
	padding: 0;
	margin: 0 20%;
